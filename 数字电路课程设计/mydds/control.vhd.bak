			WHEN"11111"=>
			control<=X"001259A1";LEDA<="0000111";LEDB<="0111111";
			WHEN OTHERS=>
			control<=X"001A36E6";LEDA<="0111111";LEDB<="0000110";
		END CASE;
	END PROCESS;
END one;