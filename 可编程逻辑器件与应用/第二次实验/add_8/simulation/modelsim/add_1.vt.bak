`timescale 100 ps/ 10 ps
module add_1_vlg_tst();
// test vector input registers
reg a;
reg b;
reg cin;
// wires                                               
wire cout;
wire sum;

// assign statements (if any)                          
add_1 i1 (
// port map - connection between master ports and signals/registers   
	.a(a),
	.b(b),
	.cin(cin),
	.cout(cout),
	.sum(sum)
);
always	#5 cin=~cin;

initial                                                
begin                                                  
A=1'b0;
B=1'b0;
cin=1'b0;
repeat(200)
	fork
		#10	B=B+1;
		#10	A=A+1;
	join
end                                                   
endmodule

